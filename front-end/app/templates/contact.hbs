



{{#paper-content classNames="md-whiteframe-z1 list-demo"}}
 {{#paper-toolbar}}

       <div class="md-toolbar-tools">
            <div class="logo">
               <strong>{{firstName}} {{lastName}}</strong>
            </div>
          </div>
           {{/paper-toolbar}}
      {{#paper-card}}
    <img src="http://miguelcobain.github.io/ember-paper/tomster.png" alt="Washed Out">
    {{#paper-card-content}}
    <h1>{{firstName}} {{lastName}}</h1>
     <dl>

  <dt>Email</dt>
  <dd>{{email}}</dd>

  <dt>Title</dt>
  <dd>{{title}}</dd>
</dl>
<p>
  Updated at: {{formatted-date updatedAt 'MMMM Do, YYYY [at] h:mm a'}}
  <br>
  Created at: {{formatted-date createdAt 'MMMM Do, YYYY [at] h:mm a'}}
</p>
    {{/paper-card-content}}
  {{/paper-card}}
  {{/paper-content}}
