{{#paper-content classNames="md-whiteframe-z1 list-demo"}}
 {{#paper-toolbar}}

       <div class="md-toolbar-tools">
            <div class="logo">
               <strong>Contacts</strong>
            </div>
          </div>
           {{/paper-toolbar}}
    {{#paper-list}}
      {{#each}}
        {{#paper-item}}
          {{#paper-tile-left}}
          {{#link-to 'contact' this}}
            <img src="http://miguelcobain.github.io/ember-paper/tomster.png" class="face">
             {{/link-to}}
          {{/paper-tile-left}}

          {{#paper-tile-content}}
            <h3>{{lastName}},</h3>
            <h4>{{firstName}}</h4>

          {{/paper-tile-content}}
        {{/paper-item}}
      {{/each}}
    {{/paper-list}}
  {{/paper-content}}
